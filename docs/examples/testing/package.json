{
  "name": "typosentinel-dashboard-tests",
  "version": "1.0.0",
  "description": "Comprehensive test suite for TypoSentinel Enhanced Web Dashboard",
  "main": "dashboard-tests.js",
  "scripts": {
    "test": "node dashboard-tests.js",
    "test:performance": "node -e \"const { PerformanceTester } = require('./dashboard-tests.js'); new PerformanceTester().runPerformanceTests()\"",
    "test:load": "node -e \"const { LoadTester } = require('./dashboard-tests.js'); new LoadTester().runLoadTests()\"",
    "test:functional": "node -e \"const { DashboardTester } = require('./dashboard-tests.js'); new DashboardTester().runAllTests()\"",
    "test:ci": "npm run test && npm run test:performance",
    "install-deps": "npm install node-fetch",
    "setup": "npm run install-deps && echo 'Test environment ready!'"
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "supertest": "^6.3.0"
  },
  "keywords": [
    "typosentinel",
    "dashboard",
    "testing",
    "security",
    "web-interface",
    "automation"
  ],
  "author": "TypoSentinel Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/typosentinel"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**"
    ]
  }
}